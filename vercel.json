{
  "version": 2,
    "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ],
  "builds": [
    { "src": "src/app.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/telegram/webhook", "dest": "src/app.js" },
    { "src": "/webhook/whatsapp", "dest": "src/app.js" },
    { "src": "/(.*)", "dest": "src/app.js" }
  ]
}
